{
  "name": "nestjs-blueprint-crud",
  "version": "1.0.4",
  "description": "A NestJS library for automatic CRUD operations with TypeORM, inspired by SailsJS Blueprint API and Waterline ORM",
  "keywords": [
    "nestjs",
    "blueprint",
    "sailsjs",
    "waterline",
    "typeorm",
    "crud",
    "orm",
    "typescript",
    "dynamic-modules",
    "query-builder",
    "associations",
    "crud-generator",
    "auto-crud",
    "api-generator",
    "sailsjs-to-nestjs",
    "migrate-sailsjs",
    "nestjs-crud",
    "nestjs-blueprint",
    "nestjs-waterline",
    "rest-api"
  ],
  "author": "law108000",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/law108000/nestjs-blueprint-crud.git"
  },
  "homepage": "https://github.com/law108000/nestjs-blueprint-crud#readme",
  "bugs": {
    "url": "https://github.com/law108000/nestjs-blueprint-crud/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint \"{src,examples}/**/*.{ts,js}\"",
    "lint:fix": "eslint \"{src,examples}/**/*.{ts,js}\" --fix",
    "format": "prettier --write \"{src,examples}/**/*.{ts,js,json,md}\" \"*.md\"",
    "format:check": "prettier --check \"{src,examples}/**/*.{ts,js,json,md}\" \"*.md\"",
    "pack": "npm pack",
    "publish:dry-run": "npm publish --dry-run",
    "db:start": "docker compose -f docker-compose.mysql.yml up -d",
    "db:stop": "docker compose -f docker-compose.mysql.yml down -v",
    "db:start:postgres": "docker compose -f docker-compose.postgres.yml up -d",
    "db:stop:postgres": "docker compose -f docker-compose.postgres.yml down -v",
    "test:e2e": "sh -c \"trap 'npm run db:stop' EXIT; npm run db:start && DB_TYPE=mysql DB_HOST=${DB_HOST:-127.0.0.1} DB_PORT=${DB_PORT:-3307} DB_USERNAME=${DB_USERNAME:-root} DB_PASSWORD=${DB_PASSWORD:-password} DB_NAME=${DB_NAME:-nestjs_crud_example} jest --config jest-e2e.config.js --runInBand --detectOpenHandles\"",
    "test:e2e:postgres": "sh -c \"trap 'npm run db:stop:postgres' EXIT; npm run db:start:postgres && DB_TYPE=postgres DB_HOST=${DB_HOST:-127.0.0.1} DB_PORT=${DB_PORT:-5433} DB_USERNAME=${DB_USERNAME:-postgres} DB_PASSWORD=${DB_PASSWORD:-password} DB_NAME=${DB_NAME:-nestjs_crud_example} jest --config jest-e2e.config.js --runInBand --detectOpenHandles\"",
    "test:e2e:sqlite": "DB_TYPE=sqlite DB_NAME=${DB_NAME:-/tmp/nestjs_crud_example.db} jest --config jest-e2e.config.js --runInBand --detectOpenHandles",
    "test:e2e:all": "npm run test:e2e && npm run test:e2e:postgres && npm run test:e2e:sqlite",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "release:patch": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0 || ^11.0.0",
    "@nestjs/core": "^10.0.0 || ^11.0.0",
    "@nestjs/swagger": "^7.0.0 || ^8.0.0",
    "class-transformer": "^0.5.0",
    "class-validator": "^0.14.0",
    "reflect-metadata": "^0.1.13 || ^0.2.0",
    "typeorm": "^0.3.0"
  },
  "devDependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/testing": "^11.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.17",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.0.0",
    "mongodb": "^6.20.0",
    "mysql2": "^2.3.3",
    "pg": "^8.16.3",
    "prettier": "^3.0.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^5.0.0",
    "sqlite3": "^5.1.7",
    "supertest": "^6.3.4",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.27",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "uuid": "^11.1.0"
  }
}
